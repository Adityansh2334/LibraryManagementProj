<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
  <welcome-file>home.html</welcome-file>
  <welcome-file>SignUp.jsp</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
  <servlet-name>IndexServ</servlet-name>
  <url-pattern>/books</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>IndexServ</servlet-name>
  <servlet-class>org.app.lib.util.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>StudServ</servlet-name>
  <url-pattern>/signupstd</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>StudServ</servlet-name>
  <servlet-class>org.app.lib.util.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>LoginServ</servlet-name>
  <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>LoginServ</servlet-name>
  <servlet-class>org.app.lib.util.SignInValidation</servlet-class>
  </servlet>
</web-app>